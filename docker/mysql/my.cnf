[mysqld]
innodb_strict_mode
#innodb_file_format = Barracuda
innodb_file_per_table
#innodb_large_prefix = 1
character-set-server=utf8mb4
#skip-character-set-client-handshake
max_allowed_packet = 32m
#skip-networking = 0
default_authentication_plugin = mysql_native_password
wait_timeout            = 86400
max_allowed_packet      = 1G
innodb_buffer_pool_size = 1G
connect_timeout = 9999
delayed_insert_timeout = 9999
innodb_flush_log_at_timeout = 9999
innodb_lock_wait_timeout = 9999
mysqlx_connect_timeout = 9999
mysqlx_idle_worker_thread_timeout  = 9999
mysqlx_port_open_timeout  = 9999
mysqlx_read_timeout = 9999
mysqlx_write_timeout = 9999
net_read_timeout = 9999
net_write_timeout = 9999
slave_net_timeout = 9999


[client]
default-character-set=utf8mb4
